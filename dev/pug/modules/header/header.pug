- var navLinks = [{name: 'Каталог', link: 'catalog.html'}, {name: 'Simple Wine Prive', link: 'prive.html'}]
- var menuLinks = [{}, {}]

header.header
    .header__logo
        a(href="index.html" target="_selft").header__logo-link
            img(src="static/images/content/logo.svg" alt="Simple Wine").header__logo-img
    nav.header__nav.nav
        ul.nav__list
            for i in navLinks
                li.nav__item
                    a(href="" +i.link).nav__link=i.name
    .header__controls.controls
        button(type="button").controls__btn.controls__btn--search
        button(type="button").controls__btn.controls__btn--wish
        button(type="button").controls__btn.controls__btn--cart
        button(type="button").controls__btn.controls__btn--login
